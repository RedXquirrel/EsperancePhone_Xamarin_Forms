<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="esperancephone.Views.DiallerButtonView">
  <Grid>
    <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
      <Label x:Name="SupLabel" FontSize="Large" HorizontalOptions="Center" TextColor="{StaticResource DiallerButtonTextColor}" />
      <Label x:Name="SubLabel" FontSize="Micro" HorizontalOptions="Center" TextColor="{StaticResource DiallerButtonTextColor}" />
    </StackLayout>
    <Label x:Name="CenterLabel" FontSize="Large" VerticalOptions="Center" HorizontalOptions="Center" TextColor="{StaticResource DiallerButtonTextColor}" />
    <Label x:Name="FontAwesomeLabel" FontSize="Large" FontFamily="FontAwesome" VerticalOptions="Center" HorizontalOptions="Center" TextColor="{StaticResource DiallerButtonTextColor}" />
  </Grid>
</ContentView>